    "@nuxtjs/auth-next": "^5.0.0-1624817847.21691f1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^3.6.5",
    "cross-fetch": "^3.1.5",
    "css-loader": "4.2.0",
    "dotenv": "^8.2.0",
    "fibers": "^5.0.0",
    "luxon": "^1.24.1",
    "nuxt": "^2.14.3",
    "nuxt-i18n": "^6.12.0",
    "scss-loader": "^0.0.1",
    "style-loader": "^1.2.1",
    "stylus-loader": "^3.0.2",
    "vue-async-computed": "^3.9.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vuedraggable": "^2.24.3",
    "vuetify": "2.3.4"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/storybook": "^3.3.1",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@storybook/addon-a11y": "^6.1.17",
    "@storybook/addon-controls": "^6.1.18",
    "@storybook/addon-docs": "^6.1.17",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^1.24.1",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/reflect-metadata": "^0.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.2",
    "babel-loader": "^8.1.0",
    "coffee-loader": "^1.0.0",
    "coffeescript": "^2.5.1",
    "concurrently": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
