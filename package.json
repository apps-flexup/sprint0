{
  "name": "sprint0",
  "version": "1.0.0",
  "description": "Version initiale du gestionnaire de contrats",
  "author": "Jonathan Beardstone",
  "private": true,
  "scripts": {
    "db": "json-server -w -p 3008 db/sample.json",
    "all": "concurrently \"yarn db\" \"yarn dev\"",
    "dev": "nuxt-ts",
    "format": "prettier --write \"**/*.{ts|js|vue}\" \"**/*.js\" \"**/*.ts\" \"**/*.vue\"",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "storybook": "concurrently \"yarn db\" \"yarn nuxt storybook\""
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.5",
    "@mdi/font": "^5.4.55",
    "@nuxt/components": "^2.2.1",
